namespace Game.Net;

struct Vec2 {
    x: float;
    y: float;
}
struct PlayerData {
    id: ubyte;
    pos: Vec2;
    angle: float;
}

struct BulletData {
    shooter: ubyte;
    pos: Vec2;
    angle: float;
}

struct GrassData {
    variety: ubyte;
    pos: Vec2;
    angle: float;
}

struct WallData {
    placer: ubyte;
    pos: Vec2;
    health: int;
}

enum CollectableType : byte {
    Coin
}

struct CollectableData {
    pos: Vec2;
    type: CollectableType;
}